                <span class="visually-hidden">Shopify</span>
                </div>
              </a>
            {%- endcapture -%}
            {{ 'general.password_page.powered_by_shopify_html' | t: shopify: shopify }}
          </div>
        </footer>
      </div>
      {% comment %}theme-check-enable MissingTemplate{% endcomment %}

    </body>

    <div x-ref="modalContent" x-show="$store.modals.modal.open">
      <template data-should-teleport="#modal-slot">
        <div data-password-login id="Login" x-show="$store.modals.modal.contents === 'password'" x-cloak class="fixed top-0 left-0 bottom-0 right-0 flex items-center text-center justify-center z-50 bg-scheme-background text-scheme-text px-section" data-color-scheme="primary" data-modal-label="{{ 'general.password_page.login_form_heading' | t }}" @click.stop x-trap.noscroll.inert="$store.modals.modal.open" role="dialog" aria-modal="true">

          <button @click="$store.modals.close('password')" class="push-btn ml-auto absolute top-2.5 right-2.5">
            <span class="visually-hidden">{{ 'general.accessibility.close_modal' | t }}</span>
            <span class="push-btn-surface align-bottom p-2">
              <span class="block w-5 h-5">{% render 'icon-close' %}</span>
            </span>
          </button>

          <div class="lg:max-w-md card-container" data-color-scheme="primary">
            <div class="card p-5 lg:p-10">
              <h1 class="font-heading heading-standard">{{ 'general.password_page.login_form_heading' | t }}</h1>

              <div class="mt-8  lg:mx-auto text-left" @modal-is-open.window="setTimeout(() => { $focus.first() }, 200)">
              {% form 'storefront_password' %}

                {% if form.errors %}
                <script type="text/javascript">
                  window.addEventListener('load', function () {
                    document.getElementById("logInWithPasswordBtn").click();
                  })
                </script>
                <div class="text-scheme-accent-1">
                {{ form.errors | default_errors }}
                </div>
                {% endif %}

                <label class="mt-4 mb-2 font-secondary text-scheme-text text-sm block" for="Password">{{ 'general.password_page.login_form_password_label' | t }}</label>
                <input type="password"
                  name="password"
                  id="Password"
                  class="input w-full"

                  >
                <button type="submit" name="commit" class="mt-5 push-btn w-full">
                  <span class="push-btn-surface w-full">{{ 'general.password_page.login_form_submit' | t }}{% if settings.buttons_show_arrow %}&nbsp;&rarr;{% endif %}</span>
                </button>
              {% endform %}
              </div>

              <p class="mt-8 text-sm">{{ 'general.password_page.admin_link_html' | t }}</p>
            </div>
            <div class="card-border"></div>
          </div>
        </div>
      </template>
    </div>



    {% render 'modal' %}

    <script src="{{ 'utils.js' | asset_url }}" defer></script>
    <script src="{{ 'global.bundle.min.js' | asset_url }}" type="module"></script>
    <script src="{{ 'modals.js' | asset_url }}" type="module"></script>
    <script src="{{ 'password.js' | asset_url }}" type="module"></script>
    <script src="{{ 'alpine-extensions.js' | asset_url }}" defer></script>


  </html>
