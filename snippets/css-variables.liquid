  /*
  Custom cursor
  */
  {%- if settings.custom_cursor != blank -%}
    {%- if settings.enable_custom_cursor_default -%}
    body {
      cursor: url('{{ settings.custom_cursor | image_url: width: '32x32' }}') {{ settings.cursor_x_position }} {{ settings.cursor_y_position }}, auto;
      cursor: -webkit-image-set(
        url('{{ settings.custom_cursor | image_url: width: '32x32' }}') 1x,
        url('{{ settings.custom_cursor | image_url: width: '64x64' }}') 2x) {{ settings.cursor_x_position }} {{ settings.cursor_y_position }}, auto;
      cursor: image-set(
        url('{{ settings.custom_cursor | image_url: width: '32x32' }}') 1x,
        url('{{ settings.custom_cursor | image_url: width: '64x64' }}') 2x) {{ settings.cursor_x_position }} {{ settings.cursor_y_position }}, auto;
    }
    {%- endif -%}
    {%- if settings.enable_custom_cursor_links -%}
    button, [role="button"], a {
        cursor: url('{{ settings.custom_cursor | image_url: width: '32x32' }}') {{ settings.cursor_x_position }} {{ settings.cursor_y_position }}, pointer;
        cursor: -webkit-image-set(
        url('{{ settings.custom_cursor | image_url: width: '32x32' }}') 1x,
        url('{{ settings.custom_cursor | image_url: width: '64x64' }}') 2x) {{ settings.cursor_x_position }} {{ settings.cursor_y_position }}, pointer;
        cursor: image-set(
        url('{{ settings.custom_cursor | image_url: width: '32x32' }}') 1x,
        url('{{ settings.custom_cursor | image_url: width: '64x64' }}') 2x) {{ settings.cursor_x_position }} {{ settings.cursor_y_position }}, pointer;
    }
